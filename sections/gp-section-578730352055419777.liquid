          {%- assign optionValue = attrs[1] -%}
          
            {% if optionKey == 'optionTitle' %}
            {% assign optionTitle = optionValue %}
            {% elsif optionKey == 'optionType' %}
              {% assign optionType = optionValue %}
            {% endif %}
          

