</div>

<script>
  const locale = 'ro-RO';

  const days = {
    'lun.': 'Lun',
    'mar.': 'Mar',
    'mie.': 'Mie',
    'joi.': 'Joi',
    'vin.': 'Vin',
    'sm.': 'Sm',
    'dum.': 'Dum'
  };

  const months = {
    'ian.': 'Ian',
    'feb.': 'Feb',
    'mar.': 'Mart',
    'apr.': 'Apr',
    'mai.': 'Mai',
    'iun.': 'Iun',
    'iul.': 'Iul',
    'aug.': 'Aug',
    'sept.': 'Sept',
    'oct.': 'Oct',
    'nov.': 'Nov',
    'dec.': 'Dec'
  };

  function formatDate(offset) {
    const d = new Date();
    d.setDate(d.getDate() + offset);
    let parts = d.toLocaleDateString(locale, { weekday: 'short', day: '2-digit', month: 'short' }).split(' ');

    const dayName = days[parts[0].toLowerCase()] || parts[0];
    const dayNumber = parts[1];
    const monthName = months[parts[2].toLowerCase()] || parts[2];

    return `${dayName} ${dayNumber} ${monthName}.`;
  }

  document.getElementById("orderDate").textContent = formatDate(0);
  document.getElementById("shippedDate").textContent = formatDate(2);
  document.getElementById("deliveredDate").textContent = formatDate(3);
</script>
