              {% for opValueItemAttr in opValueItemAttrs %}{%- assign attrs = opValueItemAttr | split: '($5)' -%}
                {%- assign opValueItemKey = attrs[0] -%}
                {%- assign opValueItemValue = attrs[1] -%}

